<div class="review">

  <div class="review-rating" data-score="<%= value.value %>"></div>



  <% if logged_in? %>
    <% if value.user_id == current_user.id %>

      <div class="links btn-group">

        <%= link_to "Edit", edit_image_value_path(value.image, value), class: "btn btn-custom" %>


      </div>

    <% end %>
  <% end %>

</div>

